{
    "name": "amorvan/responders-bundle",
    "description": "A bundle provide many responders according ADR architecture",
    "keywords": ["Symfony", "Bundle", "ADR", "Responders"],
    "homepage": "https://github.com/AurelienMo/RespondersBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurelien Morvan",
            "email": "morvan.aurelien@gmail.com",
            "homepage": "https://www.github.com/AurelienMo"
        }
    ],
    "require": {
        "php": "^7.1|^7.2|^7.3",
        "symfony/framework-bundle": "^3.4|^4",
        "twig/twig": "^1|^2",
        "symfony/serializer": "^3.4|^4.3"
    },
    "autoload": {
        "psr-4": {
            "Morvan\\Bundle\\RespondersBundle\\": ""
        },
